<h1>
  <%= @user.name %>
</h1>

<h2>Created Events:</h2>
<% if @created_events.empty? %>
<p>None</p>
<% else %>
  <% @created_events.each do |event| %>
    <div><%= link_to event.title, event %></div>
  <% end %>
<% end %>

<h2>Upcoming Attending Events:</h2>
<%= render "event_list", list: @attended_events.future %>

<h2>Past Attended Events:</h2>
<%= render "event_list", list: @attended_events.past %>